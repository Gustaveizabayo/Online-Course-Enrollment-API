{
  "name": "online-course-enrollment",
  "version": "1.0.0",
  "description": "Online Course Enrollment API with authentication, course management, and payment tracking",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:tsnd": "set PRISMA_CLIENT_ENGINE_TYPE=binary && ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "set PRISMA_CLIENT_ENGINE_TYPE=binary && jest --coverage",
    "test:watch": "set PRISMA_CLIENT_ENGINE_TYPE=binary && jest --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "set PRISMA_CLI_QUERY_ENGINE_TYPE=binary && prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "set PRISMA_CLIENT_ENGINE_TYPE=binary && ts-node src/utils/seed.ts",
    "lint": "eslint src --ext .ts",
    "prisma:fix": "set PRISMA_CLI_QUERY_ENGINE_TYPE=binary && set PRISMA_CLIENT_ENGINE_TYPE=binary && npx prisma generate"
  },
  "keywords": [
    "api",
    "course",
    "enrollment",
    "education"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^7.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}