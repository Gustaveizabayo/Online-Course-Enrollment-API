{
  "name": "online-course-enrollment",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma": "prisma",
    "prisma:generate": "prisma generate --schema ../prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema ../prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema ../prisma/schema.prisma",
    "migrate:reset": "prisma migrate reset --schema ../prisma/schema.prisma --force",
    "migrate:status": "prisma migrate status --schema ../prisma/schema.prisma",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "swagger-ui-express": "^4.7.0",
    "@prisma/client": "^5.0.0",
    "mongoose": "^8.0.0",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@types/node": "^20.11.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.4",
    "@types/passport": "^1.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2"
  }
}
